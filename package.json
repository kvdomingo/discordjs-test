{
  "name": "discordjs-test",
  "version": "0.1.0",
  "main": "src/index.ts",
  "author": "Kenneth V. Domingo <hello@kvdomingo.xyz>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "ts-node-dev .",
    "build": "wsl rm -rf build/ && tsc -p tsconfig.json",
    "prod": "node build/index.js"
  },
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "discord-api-types": "^0.27.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/preset-typescript": "^7.16.7",
    "@types/node": "^17.0.17",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-typescript"
    ],
    "minified": true
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "printWidth": 120,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
